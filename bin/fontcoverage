          0x03cb, 0x03b0, 0x03cd, 0x016f, 0x0169, 0x0076, 0x0077, 0x1e83,
          0x0175, 0x1e85, 0x2118, 0x1e81, 0x0078, 0x03be, 0x0079, 0x00fd,
          0x0177, 0x00ff, 0x00a5, 0x1ef3, 0x007a, 0x017a, 0x017e, 0x017c,
          0x0030, 0x03b6 ]

WGL4.sort()
WGL4_SUPPL.sort()
CP437.sort()
CP437_SUPPL.sort()
AGLFN.sort()

def u(codepoint):
    if codepoint < 0:
        return "%d" % codepoint
    return "U+%04X" % codepoint

def run_coverage(font, codepoint_list, name, start='', end=''):
    global args
    if args.verbose >= 1:
        print_uncovered_characters(font, codepoint_list, name, start=start, end=end)
    else:
        print_coverage_summary(font, codepoint_list, name, start=start, end=end)


def print_coverage_summary(font, codepoint_list, name, start='', end=''):
    coverage = [codepoint for codepoint in codepoint_list if codepoint in font and font[codepoint].unicode == codepoint]
    if len(coverage) > 0:
        print("    %6d of %6d = %6.2f%%  %-8s  %-8s  %s" % (
            len(coverage),
            len(codepoint_list),
            len(coverage) / len(codepoint_list) * 100,
            start,
            end,
            name
        ))

def print_uncovered_characters(font, codepoint_list, name, start='', end=''):
    coverage = [codepoint for codepoint in codepoint_list if codepoint in font and font[codepoint].unicode == codepoint]
    if len(coverage) == 0:
        return
    print("%s" % name)
    uncoverage = [codepoint for codepoint in codepoint_list if (not(codepoint in font
                                                                    and font[codepoint].unicode == codepoint)
                                                                and unicodedata_name(chr(codepoint)) is not None)]
    for codepoint in uncoverage:
        print("    %-8s  %s" % (u(codepoint), unicodedata.name(chr(codepoint))))

def unicodedata_name(chr):
    try:
        return unicodedata.name(chr)
    except (KeyError, ValueError):
        return None

main()
