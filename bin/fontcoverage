#!/usr/bin/env fontforge
# -*- mode: python; coding: utf-8 -*-
import os
import sys
import fontforge
import argparse
import unicodedata

import unicodeblocks
# note to self -- on ubuntu systems:
#     edit ~/.venv/pyvenv.cfg to set:
#         include-system-site-packages = true
#     then run:
#         sudo apt-get -y install python3-unicodeblocks

def main():
    global args
    parser = argparse.ArgumentParser()
    parser.add_argument('filenames', nargs='+')
    parser.add_argument('--verbose', '-v', action='count', default=0)
    args = parser.parse_args()
    for filename in args.filenames:
        if len(args.filenames) > 1:
            print("%s:" % filename)
        font = fontforge.open(filename)
        run_coverage(font, WGL4, "Windows Glyph List 4")
        run_coverage(font, CP437, "Code Page 437")
        run_coverage(font, AGLFN, "Adobe Glyph List for New Fonts")
        if args.verbose == 0:
            # only print unicode block info in default verbosity level of 0
            for block in unicodeblocks.blocks.values():
                run_coverage(font, range(block.start, block.end + 1), block.name, u(block.start), u(block.end))

